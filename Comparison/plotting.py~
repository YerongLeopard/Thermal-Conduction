import numpy as np
import matplotlib.pyplot as plt
import matplotlib
import pylab as pl
data= pl.loadtxt('Comparison.dat')
plt.figure(1)
x=[]
y=[]
y1=[]
y2=[]
for i in range(0,50):
  x=x+[data[i][0]]
  y=y+[data[i][1]]
  y1=y1+[data[i][2]]
  y2=y2+[data[i][3]]  
plt.scatter(x,y,s=2)
##plt.xticks(l)
plt.savefig('Comparasion.png')
plt.show()
